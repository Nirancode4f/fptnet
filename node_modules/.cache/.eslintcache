endConvsId(userId, chatData.targetId)\r\n        .then((res) => {\r\n          const convsId = res.data.conversation._id;\r\n          //Lift-state up\r\n          onCurrentConvsIdChange(convsId);\r\n\r\n          // after that get messages\r\n          if (res.data.success)\r\n            return getFriendMessages(userId, convsId, block);\r\n          else setMessages([]);\r\n        })\r\n        .then((res) => {\r\n          if (res.data.success) {\r\n            const result = res.data.messages;\r\n            setMessages(result);\r\n          } else {\r\n            setMessages([]);\r\n          }\r\n        })\r\n        .catch((err) => {\r\n          setMessages([]);\r\n          console.log(err.messages);\r\n        });\r\n    }\r\n  }, [props]);\r\n\r\n  return (\r\n    <>\r\n      <div className=\"ChatBox\">\r\n        {messages ? <ChatBoxMain messages={messages} userId={userId} /> : <></>}\r\n      </div>\r\n    </>\r\n  );\r\n}\r\n","C:\\Users\\DELL\\OneDrive\\Máy tính\\fptnet\\src\\component\\Profile\\PostPopup\\PosterTime.jsx",["304","305","306","307","308","309","310","311","312","313","314"],"import React from \"react\"\r\nimport { useState, useEffect } from \"react\";\r\nimport axios from \"axios\";\r\nimport { render } from 'react-dom';\r\nimport { Paper } from '@mui/material';\r\nimport { MenuList } from '@mui/material';\r\nimport { MenuItem } from '@mui/material';\r\nimport { ListItemIcon } from '@mui/material';\r\nimport { ListItemText } from '@mui/material';\r\nimport EditIcon from '@mui/icons-material/Edit';\r\nimport DeleteIcon from '@mui/icons-material/Delete';\r\n\r\nclass PosterTime extends React.Component{\r\n\r\n    \r\n\r\n    render() {\r\n        return (\r\n            <div className=\"modal-wiew-detail-post-info-post\">\r\n                        <div className=\"modal-wiew-detail-post-info-post-avt-user\">\r\n                            <img src={this.props.Avatar || this.props.errorPic} alt=\"\" />\r\n                        </div>